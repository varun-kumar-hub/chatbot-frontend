{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "rewrites": [
        {
            "source": "/chat",
            "destination": "/backend/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}